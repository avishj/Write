---
import "../styles/global.css";
import Head from "./Head.astro";

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
  <Head title={title} />
  <body class="min-h-screen bg-[var(--bg-base)] text-[var(--text-primary)]">
    <slot />
  </body>
</html>
